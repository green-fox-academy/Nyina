@model List<ListingToDos.Models.ToDo>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Todo CRUD App</title>
</head>
<body>
    <h1>Todos for today</h1>
    @{foreach (var element in Model)
        {
            <ul>
                <li>@element.Title</li>
            </ul>
        }
    }
</body>
</html>
